<h1>会員一覧</h1>
      <table class="table table-hover table-inverse">
          <thead>
          <tr>
          <th>会員ID</th>
          <th>氏名</th>    
          <th>メールアドレス</th>
          <th>ステータス</th>
          </tr>
          </thead>
      <tbody>
        <% @customers.each do |customer| %>
          <tr>
            <td><%= customer.id %></td>
            <td><%= customer.first_name %></td>
            <td><%= customer.email %></td>
            <td>退会済or有効</td>
          </tr>
        <% end %>
      </tbody>
      </table>