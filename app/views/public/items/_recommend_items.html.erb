
<% items.last(4).each do |item| %>
<div class="col-md-3 card center ">
<div class="mt-3">
    <%= link_to public_item_path(item.id) do %>
        <%= attachment_image_tag(item, :image, size: "250x150", fallback: "no_image.jpg") %>
    <% end%>  
</div>
<br>
<div class="card-body">
    <h6 class="card-title"><%= item.name %></h6>
    <p class="card-text"><%= "#{item.price}å††" %></p>
</div>
</div>
<% end %>


  
